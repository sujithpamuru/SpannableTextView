<?xml version="1.0" encoding="utf-8"?>
<DirectionalLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_parent"
    ohos:width="match_parent"
    ohos:orientation="vertical"
    ohos:left_padding="10fp">
    <com.mpt.android.spannabletextview.SpannableTextView
        ohos:id="$+id:line_1"
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:background_element="$graphic:background_ability_main"
        ohos:layout_alignment="horizontal_center"
        ohos:multiple_lines="true"
        ohos:text_size="10"/>
    <com.mpt.android.spannabletextview.SpannableTextView
        ohos:id="$+id:stvAddress"
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:background_element="$graphic:background_ability_main"
        ohos:layout_alignment="horizontal_center"
        ohos:multiple_lines="true"
        ohos:text_size="10"/>
    <DependentLayout
        xmlns:ohos="http://schemas.huawei.com/res/ohos"
        ohos:width="match_content"
        ohos:height="match_content">
        <com.mpt.android.spannabletextview.SpannableTextView
            ohos:id="$+id:stvAddress_1"
            ohos:width="match_content"
            ohos:height="match_content"
            ohos:left_margin="5vp"
            ohos:top_margin="5vp"
            ohos:multiple_lines="true"
            ohos:text_size="20fp"/>
        <com.mpt.android.spannabletextview.SpannableTextView
            ohos:id="$+id:stvAdd_2"
            ohos:width="match_content"
            ohos:height="match_content"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:bottom_margin="40vp"
            ohos:text_size="17fp"/>
    </DependentLayout>
        <com.mpt.android.spannabletextview.SpannableTextView
            ohos:id="$+id:line_2"
            ohos:height="match_content"
            ohos:width="match_parent"
            ohos:background_element="$graphic:background_ability_main"
            ohos:layout_alignment="horizontal_center"
        ohos:multiple_lines="true"
        ohos:text_size="10"/>
    <DependentLayout
        xmlns:ohos="http://schemas.huawei.com/res/ohos"
        ohos:width="match_content"
        ohos:height="match_content"
        ohos:background_element="$graphic:color_light_gray_element">
        <com.mpt.android.spannabletextview.SpannableTextView
            ohos:id="$+id:stvMarksTop"
            ohos:text_color="#FFF6F7F8"
            ohos:width="match_content"
            ohos:height="match_content"
            ohos:top_margin="5vp"
            ohos:bottom_margin="5vp"
            ohos:text_size="20fp"
            ohos:background_element="$graphic:background_text"/>
        <com.mpt.android.spannabletextview.SpannableTextView
            ohos:id="$+id:stvMarksTop_1"
            ohos:text_color="#073680"
            ohos:width="match_content"
            ohos:height="match_content"
            ohos:top_margin="5vp"
            ohos:bottom_margin="5vp"
            ohos:right_margin="5vp"
            ohos:text_size="20fp"
            ohos:background_element="$graphic:background_text_1"
            ohos:end_of="$id:stvMarksTop"/>
    </DependentLayout>
    <com.mpt.android.spannabletextview.SpannableTextView
        ohos:id="$+id:line_3"
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:background_element="$graphic:background_ability_main"
        ohos:layout_alignment="horizontal_center"
        ohos:multiple_lines="true"
        ohos:text_size="10"/>
    <DependentLayout
        xmlns:ohos="http://schemas.huawei.com/res/ohos"
        ohos:width="match_content"
        ohos:height="match_content"
        ohos:background_element="$graphic:full_background">
        <com.mpt.android.spannabletextview.SpannableTextView
            ohos:id="$+id:stvMarksDown"
            ohos:text_color="#FFF3F6F8"
            ohos:width="match_content"
            ohos:height="match_content"
            ohos:top_margin="5vp"
            ohos:bottom_margin="5vp"
            ohos:text_size="20fp"
            ohos:background_element="$graphic:background_horrible"/>
        <com.mpt.android.spannabletextview.SpannableTextView
            ohos:id="$+id:stvMrksDwn_1"
            ohos:text_color="#073680"
            ohos:width="match_content"
            ohos:height="match_content"
            ohos:top_margin="5vp"
            ohos:bottom_margin="5vp"
            ohos:right_margin="5vp"
            ohos:text_size="20fp"
            ohos:end_of="$id:stvMarksDown"/>
    </DependentLayout>
    <com.mpt.android.spannabletextview.SpannableTextView
