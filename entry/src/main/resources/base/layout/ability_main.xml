        ohos:layout_alignment="horizontal_center"
        ohos:multiple_lines="true"
        ohos:text_size="10"/>
    <DependentLayout
        xmlns:ohos="http://schemas.huawei.com/res/ohos"
        ohos:width="match_content"
        ohos:height="match_content">
        <com.mpt.android.spannabletextview.SpannableTextView
            ohos:id="$+id:stvFormula"
            ohos:width="match_content"
            ohos:height="match_content"
            ohos:top_margin="5vp"
            ohos:text_size="15fp"/>
        <com.mpt.android.spannabletextview.SpannableTextView
            ohos:id="$+id:stvFormula_4"
            ohos:width="match_content"
            ohos:height="match_content"
            ohos:right_margin="5vp"
            ohos:bottom_margin="20vp"
            ohos:text_size="21fp"/>
    </DependentLayout>
    <DependentLayout
        xmlns:ohos="http://schemas.huawei.com/res/ohos"
        ohos:width="match_content"
        ohos:height="match_content">
        <com.mpt.android.spannabletextview.SpannableTextView
            ohos:id="$+id:stvFormula_1"
            ohos:width="match_content"
            ohos:height="match_content"
            ohos:top_margin="5vp"
            ohos:text_size="15fp"/>
        <com.mpt.android.spannabletextview.SpannableTextView
            ohos:id="$+id:stvFormula_2"
            ohos:width="match_content"
            ohos:height="match_content"
            ohos:right_margin="5vp"
            ohos:bottom_margin="20vp"
            ohos:text_size="21fp"/>
    </DependentLayout>
    <com.mpt.android.spannabletextview.SpannableTextView
        ohos:id="$+id:line_7_8"
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:background_element="$graphic:background_ability_main"
        ohos:layout_alignment="horizontal_center"
        ohos:multiple_lines="true"
        ohos:text_size="10"/>
    <com.mpt.android.spannabletextview.SpannableTextView
        ohos:id="$+id:stvOffer"
        ohos:height="match_content"
