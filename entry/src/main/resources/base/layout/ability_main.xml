        ohos:background_element="$graphic:background_ability_main"
        ohos:layout_alignment="horizontal_center"
        ohos:multiple_lines="true"
        ohos:text_size="50"/>
    <com.mpt.android.spannabletextview.SpannableTextView
        ohos:id="$+id:line_9"
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:background_element="$graphic:background_ability_main"
        ohos:layout_alignment="horizontal_center"
        ohos:multiple_lines="true"
        ohos:text_size="10"/>
    <DependentLayout
        xmlns:ohos="http://schemas.huawei.com/res/ohos"
        ohos:width="match_content"
        ohos:height="match_content">
        <com.mpt.android.spannabletextview.SpannableTextView
        ohos:id="$+id:stvOffer_1"
        ohos:width="match_content"
        ohos:height="match_content"
        ohos:layout_alignment="horizontal_center"
        ohos:multiple_lines="true"
        ohos:bottom_margin="30vp"
        ohos:text_size="20fp"
        ohos:text_color="#0000FF"/>
    <com.mpt.android.spannabletextview.SpannableTextView
        ohos:id="$+id:stvOffer_2"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:background_element="$graphic:background_text_web"
        ohos:layout_alignment="horizontal_center"
        ohos:multiple_lines="true"
        ohos:bottom_margin="20vp"
        ohos:text_size="20fp"
        ohos:align_start="$id:stvOffer_1"/>
    </DependentLayout>
    <com.mpt.android.spannabletextview.SpannableTextView
        ohos:id="$+id:line_10"
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:background_element="$graphic:background_ability_main"
        ohos:layout_alignment="horizontal_center"
        ohos:multiple_lines="true"
        ohos:text_size="10"/>
    <DependentLayout
        xmlns:ohos="http://schemas.huawei.com/res/ohos"
        ohos:width="match_content"
        ohos:height="match_content">
        <com.mpt.android.spannabletextview.SpannableTextView
            ohos:id="$+id:stvImageSpan"
            ohos:width="match_content"
            ohos:height="match_content"
            ohos:text_font="Bold"
            ohos:text_color="#304ffe"
            ohos:top_margin="10vp"
            ohos:text_size="22fp"/>
        <com.mpt.android.spannabletextview.SpannableTextView
            ohos:id="$+id:Image"
            ohos:width="match_content"
            ohos:height="match_content"
            ohos:text_font="Bold"
            ohos:text_color="#304ffe"
            ohos:top_margin="10vp"
            ohos:text_size="22fp"/>
        <Image
            ohos:id="$+id:imagee"
            ohos:left_margin="20vp"
            ohos:width="50vp"
            ohos:height="50vp"
            ohos:layout_alignment="center"
            ohos:image_src="$media:ic_star_border"
            ohos:scale_mode="zoom_center"
            ohos:end_of="$id:stvImageSpan"/>
    </DependentLayout>
</DirectionalLayout>